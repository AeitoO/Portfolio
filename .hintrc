{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "css-prefix-order": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 117",
    "not chrome <= 117",
    "not edge <= 117",
    "not firefox <= 117",
    "not safari <= 17",
    "not samsung <= 22"
  ]
}